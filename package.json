{
  "name": "talksy",
  "version": "0.0.1",
  "description": "Real-Time AI Assistant Backend with NestJS",
  "main": "dist/main.js",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand",
    "test:verbose": "jest --runInBand --detectOpenHandles --verbose",
    "test:coverage": "jest --runInBand --coverage",
    "test:ci": "jest --runInBand --detectOpenHandles --forceExit --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --runInBand --testPathPattern=src --passWithNoTests",
    "test:integration": "jest --runInBand --testPathPattern=test/integration --passWithNoTests",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json --passWithNoTests",
    "test:e2e:docker": "API_PORT=3001 jest --runInBand --config ./test/jest-e2e.json --passWithNoTests",
    "test:api": "./scripts/test-api.sh",
    "test:api:local": "ENV=local API_URL=http://localhost:3000 ./scripts/test-api.sh",
    "test:api:docker": "ENV=docker API_URL=http://localhost:3001 ./scripts/test-api.sh",
    "test:api:vps": "ENV=vps ./scripts/test-api.sh",
    "test:api:staging": "ENV=staging ./scripts/test-api.sh",
    "test:api:production": "ENV=production ./scripts/test-api.sh",
    "test:api:verbose": "VERBOSE=true ./scripts/test-api.sh",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "docker:dev:build": "docker-compose -f docker/docker-compose.dev.yml up -d --build",
    "docker:dev:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker/docker-compose.dev.yml logs -f",
    "docker:dev:restart": "docker-compose -f docker/docker-compose.dev.yml restart",
    "docker:dev:ps": "docker-compose -f docker/docker-compose.dev.yml ps",
    "docker:test": "docker-compose -f docker/docker-compose.test.yml up -d",
    "docker:test:build": "docker-compose -f docker/docker-compose.test.yml up -d --build",
    "docker:test:down": "docker-compose -f docker/docker-compose.test.yml down",
    "docker:test:logs": "docker-compose -f docker/docker-compose.test.yml logs -f",
    "docker:test:ps": "docker-compose -f docker/docker-compose.test.yml ps",
    "docker:prod": "docker-compose -f docker/docker-compose.prod.yml up -d",
    "docker:prod:build": "docker-compose -f docker/docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker/docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker/docker-compose.prod.yml logs -f",
    "docker:prod:ps": "docker-compose -f docker/docker-compose.prod.yml ps",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "k6:local": "k6 run --env ENV=local test/k6/run-all.js",
    "k6:local:smoke": "k6 run --env ENV=local --env SMOKE=true test/k6/run-all.js",
    "k6:docker": "k6 run --env ENV=docker test/k6/run-all.js",
    "k6:docker:smoke": "k6 run --env ENV=docker --env SMOKE=true test/k6/run-all.js",
    "k6:vps": "k6 run --env ENV=vps test/k6/run-all.js",
    "k6:staging": "k6 run --env ENV=staging test/k6/run-all.js",
    "k6:production": "k6 run --env ENV=production test/k6/run-all.js",
    "k6:connection": "k6 run test/k6/scenarios/websocket-connection.js",
    "k6:messages": "k6 run test/k6/scenarios/message-flow.js",
    "k6:streaming": "k6 run test/k6/scenarios/streaming-flow.js",
    "k6:tools": "k6 run test/k6/scenarios/tool-execution.js",
    "k6:ratelimit": "k6 run test/k6/scenarios/rate-limit.js",
    "test:k6:smoke": "k6 run --vus 5 --duration 30s test/k6/scenarios/websocket-connection.js",
    "test:k6:connection": "k6 run test/k6/scenarios/websocket-connection.js",
    "test:k6:messages": "k6 run test/k6/scenarios/message-flow.js",
    "test:k6:streaming": "k6 run test/k6/scenarios/streaming-flow.js",
    "test:k6:tools": "k6 run test/k6/scenarios/tool-execution.js",
    "test:k6:ratelimit": "k6 run test/k6/scenarios/rate-limit.js",
    "test:k6:all": "k6 run test/k6/run-all.js",
    "test:ai": "node scripts/test-ai.js",
    "test:ai:hello": "node scripts/test-ai.js 'Hello, what can you do?'",
    "test:ai:capabilities": "node scripts/test-ai.js 'What are your capabilities?'",
    "infra:local:up": "docker-compose -f docker/docker-compose.test.yml up -d redis && echo 'Waiting for Redis...' && sleep 3",
    "infra:local:down": "docker-compose -f docker/docker-compose.test.yml down",
    "infra:local:status": "docker-compose -f docker/docker-compose.test.yml ps",
    "infra:docker:up": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "infra:docker:down": "docker-compose -f docker/docker-compose.dev.yml down",
    "infra:docker:status": "docker-compose -f docker/docker-compose.dev.yml ps",
    "cleanup:local": "pkill -f 'nest start' || true; pkill -f 'node dist/main' || true; docker-compose -f docker/docker-compose.test.yml down 2>/dev/null || true; rm -rf .pids .logs 2>/dev/null || true; echo 'Local environment cleaned up'",
    "cleanup:docker": "docker-compose -f docker/docker-compose.dev.yml down 2>/dev/null || true; docker-compose -f docker/docker-compose.test.yml down 2>/dev/null || true; docker-compose -f docker/docker-compose.prod.yml down 2>/dev/null || true; echo 'Docker environment cleaned up'",
    "cleanup:all": "pkill -f 'nest start' || true; pkill -f 'node dist/main' || true; pkill -f 'k6 run' || true; docker-compose -f docker/docker-compose.dev.yml down 2>/dev/null || true; docker-compose -f docker/docker-compose.test.yml down 2>/dev/null || true; docker-compose -f docker/docker-compose.prod.yml down 2>/dev/null || true; rm -rf .pids .logs 2>/dev/null || true; echo 'All environments cleaned up'",
    "test:full:local": "./scripts/test-orchestrator/run-local.sh",
    "test:full:docker": "./scripts/test-orchestrator/run-docker.sh",
    "test:full:vps": "./scripts/test-orchestrator/run-remote.sh vps",
    "test:full:staging": "./scripts/test-orchestrator/run-remote.sh staging",
    "test:full:production": "./scripts/test-orchestrator/run-remote.sh production",
    "test:jest:local": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:jest:docker": "npm run test:unit && npm run test:integration && npm run test:e2e:docker",
    "test:run:local": "./scripts/test-orchestrator/run-local.sh",
    "test:run:docker": "./scripts/test-orchestrator/run-docker.sh",
    "test:run:vps": "./scripts/test-orchestrator/run-remote.sh vps",
    "test:run:staging": "./scripts/test-orchestrator/run-remote.sh staging",
    "test:run:production": "./scripts/test-orchestrator/run-remote.sh production",
    "deploy:vps": "./scripts/deploy/deploy-remote.sh vps",
    "deploy:staging": "./scripts/deploy/deploy-remote.sh staging",
    "deploy:production": "./scripts/deploy/deploy-remote.sh production",
    "status": "./scripts/infra/status.sh"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.8.2",
    "joi": "^17.11.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "socket.io-client": "^4.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s",
      "!src/main.ts",
      "!src/**/*.module.ts",
      "!src/**/index.ts",
      "!src/config/**"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
